<div class="media">
  {{#each App.CurrentQuery.result}}
    <a class="pull-left" href="#">
      <img class="media-object" width=64 height=64 {{bindAttr src="image"}}></img>
    </a>
    <div class="media-body">
      <h4 class="media-heading">{{name}}</h4>
      {{description}}
    </div>
  {{/each}}
</div>
